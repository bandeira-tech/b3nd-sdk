  "tasks": {
    "dev": "deno run -A scripts/check-prereqs.ts && deno run --watch -A mod.ts",
    "start": "deno run -A mod.ts",
    "test": "deno test -A tests/",
    "test:unit": "deno test -A tests/test-runner.test.ts",
    "test:e2e": "deno test -A tests/e2e.test.ts"
  },
  "imports": {
    "@std/assert": "jsr:@std/assert@^1.0.15",
    "@std/path": "jsr:@std/path@^1.0.8",
    "@std/async": "jsr:@std/async@^1.0.13",
    "@std/dotenv": "jsr:@std/dotenv@^0.225.0",
    "hono": "npm:hono@^4.0.0"
  }
}
